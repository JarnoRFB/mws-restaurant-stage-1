{
    "rules": {
        "indent": [
            1,
            2
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "semi": [
            1,
            "always"
        ]
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended"
}